%h2 shift details

%ul#details
	%li
		Start Time: 
		= @shift.shiftstart
	%li
		End Time:
		= @shift.shiftend
	%li
		Shift Length:
		= (@shift.shiftend - @shift.shiftstart) / 3600
	%li
		Doctors Available:
		= @shift.users
	%li
		Shift Assigned To:
		= @shift.owner
	%li
		Shift Taken:
		= @shift.open

= link_to 'Back to all shifts', shifts_path